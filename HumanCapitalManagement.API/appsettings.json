{
  "ConnectionStrings": {
    "DefaultProdConnection": "<DATABASE_CONNECTION_STRING>"
  },
  "Logging": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Azure_Key_Vault": {
    "KeyVaultURI": "<AZURE_KEY_VAULT_URL>",
    "selectors": {
      "clientIdKey": "<AZURE_KEY_VAULT_CLIENT_ID>",
      "clientSecretKey": "<AZURE_KEY_VAULT_CLIENT_SECRET>",
      "grantTypeKey": "<AZURE_KEY_VAULT_GRANT_TYPE_KEY>"
    }
  },
  "AzureServiceBus": {
    "PrimaryConnectionString": "<AZURE_SERVICE_BUS_CONN_STRING>",
    "EmployeeQueueName": "<AZURE_SERVICE_BUS_QUEUE_NAME>"
  },
  "ApplicationInsights": {
    "ConnectionString": "<AZURE_APP_INSIGHTS_CONN_STRING>",
    "InstrumentationKey": "<AZURE_APP_INSIGHTS_INSTRUMENTATION_KEY>"
  },
  "JwtCreds": {
    "client-id": "<LOCAL_CLIENT_ID>",
    "client-secret": "<LOCAL_CLIENT_SECRET>",
    "grant-type": "<LOCAL_GRANT_TYPE>"
  },
  "AllowedHosts": "*"
}